{
  "name": "boilerplate",
  "title": "A Boilerplate for use with Lineman based on ng-boilerplate",
  "version": "0.0.1",
  "private": true,
  "licenses": {
    "type": "MIT",
    "url": "https://raw.github.com/cantoine/angular-lineman-boilerplatemaster/LICENSE"
  },
  "author": {
    "name": "Chris Antoine",
    "company": "SpinWeb"
  },
  "scripts": {
    "startServer": "node ./node_modules/.bin/actionhero start --config=./server/config/config.js",
    "startServerCluster": "node ./node_modules/.bin/actionHero startCluster --config=./server/config/config.js",
    "actionHero": "node ./node_modules/.bin/actionhero",
    "help": "node ./node_modules/.bin/actionHero help"
  },
  "dependencies": {
    "actionhero": "latest",
    "mongoose": "latest",
    "mongoose-utilities": "latest",
    "mongoose-validator": "latest",
    "passport-local-mongoose": "latest",
    "q": "latest"
  },
  "devDependencies": {
    "grunt": "latest",
    "grunt-angular-templates": "latest",
    "grunt-contrib-clean": "latest",
    "grunt-contrib-concat": "latest",
    "grunt-contrib-copy": "latest",
    "grunt-contrib-jshint": "latest",
    "grunt-contrib-less": "latest",
    "grunt-contrib-testem": "latest",
    "grunt-contrib-uglify": "latest",
    "grunt-contrib-watch": "latest",
    "grunt-debug-task": "latest",
    "grunt-mocha-test": "~0.8.2",
    "grunt-ngmin": "latest",
    "grunt-protractor-runner": "latest",
    "mocha": "latest",
    "protractor": "latest",
    "request": "latest",
    "should": "latest",
    "sinon": "latest",
    "testem": "latest"
  }
}
